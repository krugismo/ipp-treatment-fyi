  "synergyCalculation": {
    "method": "Chou-Talalay",
    "formula": "CI = Σ(Di/Dxi) + α·∏(Di/Dxi)",
    "interpretation": {
      "CI < 0.1": "Very strong synergism",
      "0.1-0.3": "Strong synergism", 
      "0.3-0.7": "Synergism",
      "0.7-0.85": "Moderate synergism",
      "0.85-0.90": "Slight synergism",
      "0.90-1.10": "Nearly additive",
      "1.10-1.20": "Slight antagonism",
      "1.20-1.45": "Moderate antagonism",
      "1.45-3.3": "Antagonism",
      "> 3.3": "Very strong antagonism"
    }
  }
